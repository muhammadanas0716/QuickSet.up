{
  "name": "quicksetup",
  "version": "0.0.1",
  "private": true,
  "description": "Ship your startup in days, not weeks - The modern boilerplate generator",
  "author": "QuickSet.up",
  "license": "MIT",
  "scripts": {
    "build": "turbo build",
